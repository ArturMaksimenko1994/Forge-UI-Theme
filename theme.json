{
  "$schema": "https://schemas.wp.org/wp/6.9/theme.json",
  "version": 2,
  "settings": {
    "layout": {
      "contentSize": "945px",
      "wideSize": "1340px"
    },
    "color": {
      "custom": false,
      "customText": false,
      "customBackground": false,
      "customGradient": false,
      "gradients": [],
      "palette": [],
      "link": false
    },
    "typography": {
      "customFontSize": false,
      "lineHeight": false,
      "letterSpacing": false,
      "textTransform": false,
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStyle": "normal",
              "fontWeight": "100 900",
              "src": [
                "file:./assets/fonts/Inter/Inter-VariableFont.ttf"
              ]
            }
          ],
          "fontFamily": "Inter",
          "name": "Inter",
          "slug": "inter"
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "14px",
          "name": "Small"
        },
        {
          "slug": "normal",
          "size": "16px",
          "name": "Normal"
        },
        {
          "slug": "medium",
          "size": "18px",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "24px",
          "name": "Large"
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "fontSize": "var(--wp--preset--font-size--normal)",
      "lineHeight": "1.6"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xxl)",
          "fontWeight": "700",
          "lineHeight": "1.2"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xl)",
          "fontWeight": "700",
          "lineHeight": "1.3"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600",
          "lineHeight": "1.4"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "1.4"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "fontWeight": "600",
          "lineHeight": "1.4"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "600",
          "lineHeight": "1.4"
        }
      },
      "p": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "fontWeight": "400",
          "lineHeight": "1.6"
        }
      },
      "ol": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "fontWeight": "400",
          "lineHeight": "1.6"
        }
      },
      "ul": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "fontWeight": "400",
          "lineHeight": "1.6"
        }
      },
      "li": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "fontWeight": "400",
          "lineHeight": "1.6"
        }
      },
      "button": {
        "typography": {
          "fontWeight": "500"
        }
      },
      "caption": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      }
    }
  }
}
